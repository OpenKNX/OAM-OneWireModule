            <ChannelIndependentBlock>
              <ParameterBlock Id="%AID%_PB-nnn" Name="General" Text="Allgemeine Parameter">
                <!-- Maximum number of Busmaster (used as Parameter for WireGateway and Sensormodul) -->
                <ParameterRefRef RefId="%AID%_P-%T%000029_R-%T%00002901" />
                <!-- the following parameter is read only and necessary (number of logik channels used in knxdprod) -->
                <ParameterRefRef RefId="%AID%_P-1_R-11" />
                <!-- startup delay -->
                <ParameterRefRef RefId="%AID%_P-2_R-21" />
                <!-- heartbeat delay -->
                <ParameterRefRef RefId="%AID%_P-3_R-31" />
                <!-- Read Time and Date on startup -->
                <ParameterRefRef RefId="%AID%_UP-4_R-41" />
                <!-- buzzer available (invisible, false) -->
                <ParameterRefRef RefId="%AID%_UP-5_R-52" />
                <!-- rgb led available (invisible, false) -->
                <ParameterRefRef RefId="%AID%_UP-6_R-62" />
                <!-- eeprom available (invisible, true) -->
                <ParameterRefRef RefId="%AID%_UP-7_R-72" />
                <choose ParamRefId="%AID%_UP-7_R-72">
                  <when test="0">
                    <ParameterSeparator Id="%AID%_PS-11" Text="Ohne EEPROM kann das Logikmodul Werte der Eingänge nicht speichern und nach einem Neustart wieder zur Verfügung stellen. Diese Funktion wird in der Applikation nicht angeboten." UIHint="Information" />
                  </when>
                </choose>
                <!-- ncn5130 available (invisible, true) -->
                <ParameterRefRef RefId="%AID%_UP-8_R-82" />
                <choose ParamRefId="%AID%_UP-8_R-82">
                  <when test="0">
                    <ParameterSeparator Id="%AID%_PS-12" Text="Ohne Abschaltmöglichkeit der Zusatzhardware (RGB-LED, Buzzer) kann das Logikmodul Werte der Eingänge nicht speichern und nach einem Neustart wieder zur Verfügung stellen. Diese Funktion wird in der Applikation nicht angeboten." UIHint="Information" />
                  </when>
                </choose>
                <ComObjectRefRef RefId="%AID%_O-2_R-21" />
                <ComObjectRefRef RefId="%AID%_O-3_R-31" />
                <choose ParamRefId="%AID%_P-3_R-31">
                  <when test="!=0">
                    <!-- show heartbeat only if it is parametrized -->
                    <ComObjectRefRef RefId="%AID%_O-1_R-11" />
                  </when>
                </choose>
                <!-- move locally set values to global parameters -->
                <!-- this is used for decoupling specific applications from global behaviour -->
                <Assign TargetParamRefRef="%AID%_UP-5_R-51" SourceParamRefRef="%AID%_UP-5_R-52"/>
                <Assign TargetParamRefRef="%AID%_UP-6_R-61" SourceParamRefRef="%AID%_UP-6_R-62"/>
                <Assign TargetParamRefRef="%AID%_UP-7_R-71" SourceParamRefRef="%AID%_UP-7_R-72"/>
              </ParameterBlock>
            </ChannelIndependentBlock>
            <Channel Id="%AID%_CH-6" Name="BM" Number="6" Text="1-Wire-Bus" >
              <ParameterBlock Id="%AID%_PB-1" Name="BM" Text="Busmaster-Einstellungen">
                <!-- Calculated number of 1-Wire-Devices supported -->
                <ParameterSeparator Id="%AID%_PS-11" Text="Kanäle und Busmaster" UIHint="Headline" />
                <ParameterRefRef RefId="%AID%_P-%T%000028_R-%T%00002801" />
                <choose ParamRefId="%AID%_P-%T%000029_R-%T%00002901">
                  <when test="1">
                    <Assign TargetParamRefRef="%AID%_UP-%T%000001_R-%T%00000101" Value="1" />
                  </when>
                  <when test="3">
                    <ParameterRefRef RefId="%AID%_UP-%T%000001_R-%T%00000101" />
                    <ParameterSeparator Id="%AID%_PS-13" Text="" UIHint="HorizontalRuler" />
                  </when>
                </choose>
                <choose ParamRefId="%AID%_UP-%T%000001_R-%T%00000101">
                  <when test="1">
                    <!-- one Busmaster supports 30 Devices -->
                    <Assign TargetParamRefRef="%AID%_P-%T%000028_R-%T%00002801" Value="30" />
                  </when>
                  <when test="2">
                    <!-- two Busmaster support 60 Devices -->
                    <Assign TargetParamRefRef="%AID%_P-%T%000028_R-%T%00002801" Value="60" />
                  </when>
                  <when test="3">
                    <!-- three Busmaster support 90 Devices -->
                    <Assign TargetParamRefRef="%AID%_P-%T%000028_R-%T%00002801" Value="90" />
                  </when>
                </choose>
                <ParameterSeparator Id="%AID%_PS-11" Text="Gerätesuche" UIHint="Headline" />
                <ParameterRefRef RefId="%AID%_UP-%T%000002_R-%T%00000201" />
                <choose ParamRefId="%AID%_UP-%T%000002_R-%T%00000201">
                  <when test="1">
                    <ComObjectRefRef RefId="%AID%_O-%T%000020_R-%T%00002001" />
                    <ParameterSeparator Id="%AID%_PS-16" Text="Im Produktivbetrieb sollte die Gerätesuche immer aus sein, da sie bei hoher KNX-Buslast dazu führen kann, dass Telegramme verloren gehen. Dieser Paremter sollte nur an sein, wenn man neue 1-Wire-Geräte anschließen will." UIHint="Information" />
                  </when>
                </choose>
                <ParameterSeparator Id="%AID%_PS-14" Text="" UIHint="HorizontalRuler" />
                <ParameterSeparator Id="%AID%_PS-11" Text="Fehlermeldungen" UIHint="Headline" />
                <ParameterRefRef RefId="%AID%_UP-%T%000000_R-%T%00000001" />
                <choose ParamRefId="%AID%_UP-%T%000000_R-%T%00000001">
                  <when test="1">
                    <!-- show error object only if it is parametrized -->
                    <ComObjectRefRef RefId="%AID%_O-%T%000021_R-%T%00002101" />
                    <ParameterSeparator Id="%AID%_PS-1" Text="Das Fehlerobjekt KO-21 ist eine Bitleiste, bei der ein Bit auf 1 gesetzt wird, wenn das entsprechende Gerät einen Fehler aufweist:" />
                    <ParameterSeparator Id="%AID%_PS-2" Text="  Bit 0: Fehler vom Busmaster selbst" />
                    <ParameterSeparator Id="%AID%_PS-3" Text="  Bit 1-30: Fehler von Gerät 1 bis 30" />
                    <choose ParamRefId="%AID%_UP-%T%000001_R-%T%00000101">
                      <when test="2 3">
                        <ComObjectRefRef RefId="%AID%_O-%T%000022_R-%T%00002201" />
                        <ParameterSeparator Id="%AID%_PS-4" Text="Das gilt für das Fehlerobjekt KO-22 anlaog. " />
                        <ParameterSeparator Id="%AID%_PS-5" Text="  Bit 0: Fehler vom Busmaster 2 selbst" />
                        <ParameterSeparator Id="%AID%_PS-6" Text="  Bit 1-30: Fehler von Gerät 31 bis 60" />
                      </when>
                      <when test="3">
                        <ComObjectRefRef RefId="%AID%_O-%T%000022_R-%T%00002201" />
                        <ComObjectRefRef RefId="%AID%_O-%T%000023_R-%T%00002301" />
                        <ParameterSeparator Id="%AID%_PS-15" Text="Und auch für das Fehlerobjekt KO-23." />
                        <ParameterSeparator Id="%AID%_PS-7" Text="  Bit 0: Fehler vom Busmaster 3 selbst" />
                        <ParameterSeparator Id="%AID%_PS-8" Text="  Bit 1-30: Fehler von Gerät 61 bis 90" />
                      </when>
                    </choose>
                  </when>
                </choose>
              </ParameterBlock>
              <ParameterBlock Id="%AID%_PB-2" Name="IButtonGroups" Text="iButton Gruppierung">
                <ParameterSeparator Id="%AID%_PS-25" Text="iButton-Gruppen" UIHint="Headline" />
